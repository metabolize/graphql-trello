{
  "name": "trello-apollo",
  "version": "0.0.3",
  "description": "GraphQL interface to the Trello REST API",
  "author": "Metabolize",
  "license": "MIT",
  "repository": "metabolize/trello-apollo",
  "main": "lib",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "prettier": "prettier --write \"**/*.@(yml|json|js|md)\"",
    "prettier:check": "prettier --check \"**/*.@(yml|json|js|md)\"",
    "example-get-board": "node examples/get-board",
    "example-get-list": "node examples/get-list",
